{
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["import", "jsx-a11y", "react", "react-hooks"],
  "rules": {
    "no-unused-vars": ["warn"],
    "no-underscore-dangle": ["warn"]
  }
}
