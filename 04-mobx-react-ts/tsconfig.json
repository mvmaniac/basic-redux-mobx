{
  "compilerOptions": {
    "strict": true,
    "target": "ES2015",
    "lib": ["ES2020", "DOM"],
    "outDir": "dist",
    "jsx": "react",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "esModuleInterop": true // mobx-react 에서 해당 옵션을 켜고 했기 때문에 옵션을 넣음
  },
  "include": ["src/**/*.tsx", "src/**/*.ts", "webpack.config.js"]
}
